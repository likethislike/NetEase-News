<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="line">
    <!--显示虚线,破折线的宽度为dashWith,空隙的宽度为dashGap,darkgray-->
    <stroke
        android:width="2dp"
        android:color="#000"
        android:dashGap="20dp"
        android:dashWidth="3dp"
        />
    <size android:height="10dp"
        android:width="200dp"
        />
</shape>